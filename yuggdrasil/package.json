  "main": "./dist/index.js",
  "module": "./src/index.ts",
  "bin": "./dist/bin.js",
  "engines": {
    "node": "11.14.0",
    "npm": "6.7.0"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "npm run test && codecov",
    "clean": "rm -rf dist",
    "dev": "npm run clean && npm run build && concurrently 'npm run build:scripts:watch' 'npm run bundle:scripts:watch' 'npm run server:watch'",
    "build": "npm run clean && npm run build:scripts && NODE_ENV=production npm run bundle:scripts",
    "build:watch": "npm run clean && concurrently 'npm run build:scripts:watch' 'npm run bundle:scripts:watch'",
    "build:scripts": "tsc -p tsconfig.prod.json -d",
    "build:scripts:watch": "tsc -w -p tsconfig.json -d",
    "bundle:scripts": "webpack --mode production",
    "bundle:scripts:watch": "webpack -w --mode development",
    "server": "node dist/bin.js",
    "server:watch": "nodemon -w ./dist -x 'node dist/bin.js'",
    "lint": "eslint '{src/**/,cli/**/,}*.{ts,tsx,js,jsx,json}'",
    "lint:fix": "eslint --fix '{src/**/,cli/**/,}*.{ts,tsx,js,jsx,json}'",
    "check": "npx npm-check-updates"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
