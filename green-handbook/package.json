{
  "name": "green-handbook",
  "version": "1.0.0",
  "description": "緑色手帖",
  "main": "index.js",
  "scripts": {
    "pre": "(cd ../publisher && npm run build)",
    "link": "(cd ../publisher && npm link) && npm link publisher",
    "dev": "rm -rf dist && npm run link && webpack --mode development && concurrently 'ts-node ./cli/dev.ts' 'webpack -w --mode development'",
    "build": "rm -rf dist && npm run link && webpack --mode production && ts-node ./cli/build.ts",
    "publisher": "npm run link && nodemon -w ../publisher/dist -x publisher",
    "server": "static -p 8000 ./dist",
    "lint": "eslint '{src/**/,cli/**/,}*.{ts,tsx,js,jsx,json}'",
    "lint:fix": "eslint --fix '{src/**/,cli/**/,}*.{ts,tsx,js,jsx,json}'",
    "check": "npx npm-check-updates"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "8.0.0",
    "@types/node": "12.6.2",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "@types/react-intl": "2.3.18",
    "@types/react-redux": "7.1.1",
    "@types/react-router-dom": "4.3.4",
    "@types/redux-thunk": "2.1.0",
    "@types/styled-components": "4.1.18",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "concurrently": "4.1.1",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "express": "4.17.1",
    "fs-extra": "8.1.0",
    "node-static": "0.7.11",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "typescript": "3.5.3",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-manifest-plugin": "2.0.4"
  },
  "dependencies": {
    "@loadable/component": "5.10.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-intl": "2.9.0",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "styled-components": "4.3.2"
  }
}
